{
  "name": "football-dashboard-backend",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch server/_core/index.ts",
    "standalone": "NODE_ENV=development tsx watch server/standalone.ts",
    "build": "esbuild server/_core/index.prod.ts --platform=node --packages=external --bundle --format=esm --outfile=../dist/index.js",
    "build:standalone": "esbuild server/standalone.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cd ../dist && NODE_ENV=production node index.js",
    "test": "vitest run",
    "db:push": "drizzle-kit generate && drizzle-kit migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.693.0",
    "@trpc/server": "^11.6.0",
    "axios": "^1.12.0",
    "better-sqlite3": "^12.6.2",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "jose": "6.1.0",
    "mysql2": "^3.15.0",
    "nanoid": "^5.1.5",
    "superjson": "^1.13.3",
    "ws": "^8.19.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@builder.io/vite-plugin-jsx-loc": "^0.1.1",
    "@tailwindcss/vite": "^4.1.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "4.17.21",
    "@types/node": "^24.7.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-manus-runtime": "^0.0.57",
    "vitest": "^2.1.4"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
