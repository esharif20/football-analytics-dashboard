# Football Analytics Dashboard - Environment Configuration
# Copy this file to .env and fill in values as needed
#
# REQUIRED for local development:
LOCAL_DEV_MODE=true
DATABASE_URL=mysql://root:football123@localhost:3307/football_dashboard
JWT_SECRET=local-dev-secret-change-in-production
LOCAL_STORAGE_DIR=./uploads

# OPTIONAL: OpenAI API key for AI commentary feature
# Get one at: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# OPTIONAL: Roboflow API key for pitch detection
# Get one at: https://app.roboflow.com/settings/api
ROBOFLOW_API_KEY=

# Leave these empty for local development (Manus services disabled)
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=
OAUTH_SERVER_URL=
VITE_APP_ID=
VITE_OAUTH_PORTAL_URL=
VITE_APP_TITLE=Football Analytics
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=
